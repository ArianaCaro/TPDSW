{
  "compilerOptions": {
      "incremental": true,
      "target": "es2016",
      "module": "commonjs",
      "sourceMap": true,
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "alwaysStrict": true,
      "skipLibCheck": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true
  },
  "include": ["src/**/*","**/*.ts"],
  "exclude": ["node_modules"]
}